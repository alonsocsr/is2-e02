{
  "builds": [
    {
        "src": "cms/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
    "config": {
        "distDir": "/staticfiles"
    }
    }
  ],
  "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
          "src": "/(.*)",
          "dest": "cms/wsgi.py"
      }
  ],
   "outputDirectory": "/staticfiles"
}